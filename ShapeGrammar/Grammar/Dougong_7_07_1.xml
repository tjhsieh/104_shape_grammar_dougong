	<rules>
    <rule>
      <description>由空間原點放入櫨枓</description>
      <from></from>
      <to>RD</to>
    </rule>
    <rule>
      <description>從櫨枓插入泥道栱</description>
      <from>RD</from>
      <to>RD=NDG(*,*)</to>
    </rule>
    <rule>
      <description>從泥道栱插入散枓(左)</description>
      <from>NDG(*,*)</from>
      <to>NDG(@[0|90|0]SD,*)</to>
    </rule>
    <rule>
      <description>從泥道栱插入散枓(右)</description>
      <from>NDG(*,*)</from>
      <to>NDG(*,@[0|90|0]SD)</to>
    </rule>
    <rule>
      <description>從華栱插入交互枓(左)</description>
      <from>@[0|90|0]HG(*,*)</from>
      <to>@[0|90|0]HG(@[0|90|0]JHD,*)</to>
    </rule>
    <rule>
      <description>從華栱插入交互枓(右)</description>
      <from>@[0|90|0]HG(*,*)</from>
      <to>@[0|90|0]HG(*,@[0|90|0]JHD)</to>
    </rule>
    <rule>
      <description>從泥道栱插入華栱</description>
      <from>NDG(*,*)</from>
      <to>NDG(*,*)=@[0|90|0]HG(*,*)</to>
    </rule>
    <rule>
      <description>從交互枓(左)插入瓜子栱</description>
      <from>@[0|90|0]HG(@[0|90|0]JHD,*)</from>
      <to>@[0|90|0]HG(@[0|90|0]JHD=GJG(*,*),*)</to>
    </rule>
    <rule>
      <description>從交互枓(右)插入瓜子栱</description>
      <from>@[0|90|0]HG(*,@[0|90|0]JHD)</from>
      <to>@[0|90|0]HG(*,@[0|90|0]JHD=GJG(*,*))</to>
    </rule>
    <rule>
      <description>從華栱插入慢拱</description>
      <from>@[0|90|0]HG(*,*)</from>
      <to>@[0|90|0]HG(*,*)=MG(*,*)</to>
    </rule>
    <rule>
      <description>從瓜子栱插入散枓(左)</description>
      <from>GJG(*,*)</from>
      <to>GJG(@[0|90|0]SD,*)</to>
    </rule>
    <rule>
      <description>從瓜子栱插入散枓(右)</description>
      <from>GJG(*,*)</from>
      <to>GJG(*,@[0|90|0]SD)</to>
    </rule>
    <rule>
      <description>從慢拱插入散枓(左)</description>
      <from>MG(*,*)</from>
      <to>MG(@[0|90|0]SD,*)</to>
    </rule>
    <rule>
      <description>從慢拱插入散枓(右)</description>
      <from>MG(*,*)</from>
      <to>MG(*,@[0|90|0]SD)</to>
    </rule>
    <rule>
      <description>從華栱插入騎槽檐栱A</description>
      <from>@[0|90|0]HG(*,*)=MG(*,*)</from>
      <to>@[0|90|0]HG(*,*)=MG(*,*)=@[0|90|0]CTJG_A(*,*,*,*)</to>
    </rule>
    <rule>
      <description>從騎槽檐栱A插入交互枓(左)</description>
      <from>@[0|90|0]CTJG_A(*,*,*,*)</from>
      <to>@[0|90|0]CTJG_A(@[0|90|0]JHD,*,*,*)</to>
    </rule>
    <rule>
      <description>從騎槽檐栱A插入慢栱(左)</description>
      <from>@[0|90|0]CTJG_A(*,*,*,*)</from>
      <to>@[0|90|0]CTJG_A(*,MG(*,*),*,*)</to>
    </rule>
    <rule>
      <description>從騎槽檐栱A插入柱頭方</description>
      <from>@[0|90|0]CTJG_A(*,*,*,*)</from>
      <to>@[0|90|0]CTJG_A(*,*,*,*)=JTF(*,*)</to>
    </rule>
    <rule>
      <description>從柱頭方插入散枓(左)</description>
      <from>@[0|90|0]CTJG_A(*,*,*,*)=JTF(*,*)</from>
      <to>@[0|90|0]CTJG_A(*,*,*,*)=JTF(@[0|90|0]SD,*)</to>
    </rule>
    <rule>
      <description>從柱頭方插入散枓(右)</description>
      <from>@[0|90|0]CTJG_A(*,*,*,*)=JTF(*,*)</from>
      <to>@[0|90|0]CTJG_A(*,*,*,*)=JTF(*,@[0|90|0]SD)</to>
    </rule>
    <rule>
      <description>從騎槽檐栱A插入交互枓(右)</description>
      <from>@[0|90|0]CTJG_A(*,*,*,*)</from>
      <to>@[0|90|0]CTJG_A(*,*,*,@[0|90|0]JHD)</to>
    </rule>
    <rule>
      <description>從騎槽檐栱A插入慢栱(右)</description>
      <from>@[0|90|0]CTJG_A(*,*,*,*)</from>
      <to>@[0|90|0]CTJG_A(*,*,MG(*,*),*)</to>
    </rule>
    <rule>
      <description>從交互枓插入令栱</description>
      <from>@[0|90|0]CTJG_A(@[0|90|0]JHD,*,*,*)</from>
      <to>@[0|90|0]CTJG_A(@[0|90|0]JHD=LG(*,*),*,*,*)</to>
    </rule>
    <rule>
      <description>從交互枓插入令栱</description>
      <from>@[0|90|0]CTJG_A(*,*,*,@[0|90|0]JHD)</from>
      <to>@[0|90|0]CTJG_A(*,*,*,@[0|90|0]JHD=LG(*,*))</to>
    </rule>
    <rule>
      <description>從令栱插入散枓(左)</description>
      <from>LG(*,*)</from>
      <to>LG(@[0|90|0]SD,*)</to>
    </rule>
    <rule>
      <description>從令栱插入散枓(右)</description>
      <from>LG(*,*)</from>
      <to>LG(*,@[0|90|0]SD)</to>
    </rule>
    <rule>
      <description>從令栱插入騎槽檐栱B</description>
      <from>@[0|90|0]CTJG_A(*,*,*,@[0|90|0]JHD=LG(*,*))</from>
      <to>@[0|90|0]CTJG_A(*,*,*,@[0|90|0]JHD=LG(*,*)=CTJG_B(*,*,*))</to>
    </rule>
    <rule>
      <description>從騎槽檐栱B插入交互枓</description>
      <from>CTJG_B(*,*,*)</from>
      <to>CTJG_B(@[0|90|0]JHD,*,*)</to>
    </rule>
    <rule>
      <description>從騎槽檐栱B插入慢栱</description>
      <from>CTJG_B(*,*,*)</from>
      <to>CTJG_B(*,*,*)=MG(*,*)</to>
    </rule>
    <rule>
      <description>從騎槽檐栱B插入柱頭方</description>
      <from>CTJG_B(*,*,*)</from>
      <to>CTJG_B(*,JTF,*)</to>
    </rule>
    <rule>
      <description>從騎槽檐栱B插入柱頭方</description>
      <from>CTJG_B(*,*,*)</from>
      <to>CTJG_B(*,*,JTF)</to>
    </rule>
    <rule>
      <description>從交互枓插入瓜子栱</description>
      <from>CTJG_B(*,*,*)</from>
      <to>CTJG_B(@[0|90|0]JHD=GJG(*,*),*,*)</to>
    </rule>
    <rule>
      <description>從瓜子栱插入裏耍頭(左)</description>
      <from>CTJG_B(@[0|90|0]JHD=GJG(*,*),*,*)</from>
      <to>CTJG_B(@[0|90|0]JHD=GJG(*,*)=LSTL(*),*,*)</to>
    </rule>
    <rule>
      <description>從裏耍頭(左)插入柱頭方</description>
      <from>LSTL(*)</from>
      <to>LSTL(JTF)</to>
    </rule>
    <rule>
      <description>從裏耍頭(左)插入齊心枓</description>
      <from>LSTL(*)</from>
      <to>LSTL(*)=@[0|90|0]QXD</to>
    </rule>
    <rule>
      <description>從齊心枓插入平棊方</description>
      <from>LSTL(*)=@[0|90|0]QXD</from>
      <to>LSTL(*)=@[0|90|0]QXD=PCF</to>
    </rule>
    <rule>
      <description>從騎槽檐栱B插入昂</description>
      <from>@[0|90|0]CTJG_A(@[0|90|0]JHD=LG(*,*),*,*,@[0|90|0]JHD=LG(*,*)=CTJG_B(*,*,*))</from>
      <to>@[0|90|0]CTJG_A(@[0|90|0]JHD=LG(*,*)=ON(*,*,*,*,*),*,*,@[0|90|0]JHD=LG(*,*)=CTJG_B(*,*,*))</to>
