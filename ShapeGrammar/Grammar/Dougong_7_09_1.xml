	<rules>
    <rule>
      <description>由空間原點放入櫨枓</description>
      <from></from>
      <to>RD</to>
    </rule>
    <rule>
      <description>從櫨枓插入泥道栱</description>
      <from>RD</from>
      <to>RD=NDG(*,*)</to>
    </rule>
    <rule>
      <description>從泥道栱插入散枓(左)</description>
      <from>NDG(*,*)</from>
      <to>NDG(@[0|90|0]SD,*)</to>
    </rule>
    <rule>
      <description>從泥道栱插入散枓(右)</description>
      <from>NDG(*,*)</from>
      <to>NDG(*,@[0|90|0]SD)</to>
    </rule>
    <rule>
      <description>從泥道栱插入華栱</description>
      <from>NDG(*,*)</from>
      <to>NDG(*,*)=@[0|90|0]HG(*,*)</to>
    </rule>
    <rule>
      <description>從華栱插入交互枓(左)</description>
      <from>HG(*,*)</from>
      <to>HG(@[0|90|0]JHD,*)</to>
    </rule>
    <rule>
      <description>從華栱插入散枓(右)</description>
      <from>HG(*,*)</from>
      <to>HG(*,SD)</to>
    </rule>
    <rule>
      <description>從交互枓插入瓜子栱</description>
      <from>@[0|90|0]JHD</from>
      <to>@[0|90|0]JHD=GJG(*,*)</to>
    </rule>
    <rule>
      <description>從華栱插入慢栱</description>
      <from>HG(*,*)</from>
      <to>HG(*,*)=MG(*,*)</to>
    </rule>
    <rule>
      <description>從瓜子栱插入散枓(右)</description>
      <from>GJG(*,*)</from>
      <to>GJG(*,@[0|90|0]SD)</to>
    </rule>
    <rule>
      <description>從瓜子栱插入散枓(左)</description>
      <from>GJG(*,*)</from>
      <to>GJG(@[0|90|0]SD,*)</to>
    </rule>
    <rule>
      <description>從慢栱插入散枓(左)</description>
      <from>MG(*,*)</from>
      <to>MG(@[0|90|0]SD,*)</to>
    </rule>
    <rule>
      <description>從慢栱插入散枓(左)</description>
      <from>MG(*,*)</from>
      <to>MG(*,@[0|90|0]SD)</to>
    </rule>
    <rule>
      <description>從慢栱插入華栱</description>
      <from>MG(*,*)</from>
      <to>MG(*,*)=HG_2(*,*,*)</to>
    </rule>
    <rule>
      <description>從華栱_2插入交互枓(左)</description>
      <from>HG_2(*,*,*)</from>
      <to>HG_2(@[0|90|0]JHD,*,*)</to>
    </rule>
    <rule>
      <description>從華栱_2插入連珠枓(右)</description>
      <from>HG_2(*,*,*)</from>
      <to>HG_2(*,*,LJD)</to>
    </rule>
    <rule>
      <description>從華栱_2插入慢栱</description>
      <from>HG_2(*,*,*)</from>
      <to>HG_2(*,MG_2(*,*),*)</to>
    </rule>
    <rule>
      <description>從華栱_2插入柱頭方</description>
      <from>HG_2(*,*,*)</from>
      <to>HG_2(*,*,*)=JTF</to>
    </rule>
  <rule>
      <description>從慢栱插入散枓(左)</description>
      <from>MG_2(*,*)</from>
      <to>MG_2(@[0|90|0]SD,*)</to>
    </rule>
  <rule>
      <description>從慢栱插入散枓(右)</description>
      <from>MG_2(*,*)</from>
      <to>MG_2(*,@[0|90|0]SD)</to>
    </rule>
  <rule>
      <description>從瓜子栱插入騎槽檐栱</description>
      <from>HG_2(@[0|90|0]JHD=GJG(*,*),MG_2(*,*),*)</from>
      <to>HG_2(@[0|90|0]JHD=GJG(*,*)=CTJG(*,*),MG_2(*,*),*)</to>
    </rule>
  <rule>
      <description>從連珠枓插入上昂1</description>
      <from>LJD</from>
      <to>LJD=UPON_1(*)</to>
    </rule>
  <rule>
      <description>從柱頭方插入齊心枓</description>
      <from>HG_2(*,*,*)=JTF</from>
      <to> HG_2(*,*,*)=JTF=@[0|90|0]QXD</to>
    </rule>
  <rule>
      <description>從上昂1插入散枓</description>
      <from>UPON_1(*)</from>
      <to>UPON_1(SD)</to>
    </rule>
  <rule>
      <description>從上昂1插入瓜子栱</description>
      <from>UPON_1(*)</from>
      <to>UPON_1(*)=GJG(*,*)</to>
    </rule>
   <rule>
      <description>從騎槽檐栱插入交互枓</description>
      <from>CTJG(*,*)</from>
      <to>CTJG(@[0|90|0]JHD,*)</to>
    </rule>
  <rule>
      <description>從上昂1瓜子栱插入上昂2</description>
      <from>UPON_1(*)=GJG(*,*)</from>
      <to>UPON_1(*)=GJG(*,*)=UPON_2(*)</to>
    </rule>
  <rule>
      <description>從上昂2插入交互枓</description>
      <from>UPON_2(*)</from>
      <to>UPON_2(@[0|90|0]JHD)</to>
    </rule>
  <rule>
      <description>從上昂2插入齊心枓</description>
      <from>UPON_2(*)</from>
      <to>UPON_2(*)=@[0|90|0]QXD</to>
    </rule>
  <rule>
      <description>從騎槽檐栱插入慢栱</description>
      <from>CTJG(*,*)</from>
      <to>CTJG(*,*)=MG_2(*,*)</to>
    </rule>
  <rule>
      <description>從騎槽檐栱插入住頭方</description>
      <from>CTJG(*,*)</from>
      <to>CTJG(*,JTF)</to>
    </rule>
  <rule>
      <description>從騎槽檐栱上慢栱插入耍頭L</description>
      <from>CTJG(*,*)=MG_2(*,*)</from>
      <to>CTJG(*,*)=MG_2(*,*)=STL(*,*,*)</to>
    </rule>
  <rule>
      <description>從住頭方齊心枓插入瓜子栱</description>
      <from>HG_2(*,*,*)=JTF=@[0|90|0]QXD</from>
      <to>HG_2(*,*,*)=JTF=@[0|90|0]QXD=GJG(*,*)</to>
    </rule>
	<rule>
      <description>從耍頭L插入齊心枓</description>
      <from>STL(*,*,*)</from>
      <to>STL(@[0|90|0]QXD,*,*)</to>
    </rule>
	<rule>
      <description>從耍頭L齊心枓插入平棊方</description>
      <from>STL(@[0|90|0]QXD,*,*)</from>
      <to>STL(@[0|90|0]QXD=PCF,*,*)</to>
    </rule>
	<rule>
      <description>從耍頭L插入柱頭方L</description>
      <from>STL(*,*,*)</from>
      <to>STL(*,*,*)=JTF</to>
    </rule>
	<rule>
      <description>從耍頭L插入柱頭方R</description>
      <from>STL(*,*,*)</from>
      <to>STL(*,*,JTF)</to>
    </rule>
	<rule>
      <description>從上昂2齊心枓插入慢栱</description>
      <from>UPON_2(*)=@[0|90|0]QXD</from>
      <to>UPON_2(*)=@[0|90|0]QXD=MG_2(*,*)</to>
    </rule>
	<rule>
      <description>從耍頭L插入襯方頭</description>
      <from>STL(*,*,*)</from>
      <to>STL(*,CFT(*),*)</to>
    </rule>
	<rule>
      <description>從襯方頭插入齊心枓</description>
      <from>CFT(*)</from>
      <to>CFT(@[0|90|0]QXD)</to>
    </rule>
	<rule>
      <description>從襯方頭齊心枓插入柱頭方</description>
