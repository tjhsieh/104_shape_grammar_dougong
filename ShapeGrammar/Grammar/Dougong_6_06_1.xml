	<rules>
    <rule>
      <description>由空間原點放入櫨枓</description>
      <from></from>
      <to>RD</to>
    </rule>
    <rule>
      <description>從櫨枓插入泥道栱</description>
      <from>RD</from>
      <to>RD=NDG(*,*)</to>
    </rule>
    <rule>
      <description>從泥道栱插入華栱</description>
      <from>NDG(*,*)</from>
      <to>NDG(*,*)=@[0|90|0]HG(*,*)</to>
    </rule>
    <rule>
      <description>從泥道栱插入散枓(左)</description>
      <from>NDG(*,*)</from>
      <to>NDG(@[0|90|0]SD,*)</to>
    </rule>
    <rule>
      <description>從泥道栱插入散枓(右)</description>
      <from>NDG(*,*)</from>
      <to>NDG(*,@[0|90|0]SD)</to>
    </rule>
    <rule>
      <description>從華栱插入交互枓(左)</description>
      <from>@[0|90|0]HG(*,*)</from>
      <to>@[0|90|0]HG(@[0|90|0]JHD,*)</to>
    </rule>
    <rule>
      <description>從華栱插入齊心枓(右)</description>
      <from>@[0|90|0]HG(*,*)</from>
      <to>@[0|90|0]HG(*,QXD)</to>
    </rule>
    <rule>
      <description>從交互枓插入瓜子栱</description>
      <from>@[0|90|0]HG(@[0|90|0]JHD,*)</from>
      <to>@[0|90|0]HG(@[0|90|0]JHD=GJG(*,*),*)</to>
    </rule>
    <rule>
      <description>從華栱插入慢拱</description>
      <from>@[0|90|0]HG(*,*)</from>
      <to>@[0|90|0]HG(*,*)=MG(*,*)</to>
    </rule>
    <rule>
      <description>從瓜子栱插入散枓(左)</description>
      <from>GJG(*,*)</from>
      <to>GJG(@[0|90|0]SD,*)</to>
    </rule>
    <rule>
      <description>從瓜子栱插入散枓(右)</description>
      <from>GJG(*,*)</from>
      <to>GJG(*,@[0|90|0]SD)</to>
    </rule>
    <rule>
      <description>從慢栱插入散枓(左)</description>
      <from>MG(*,*)</from>
      <to>MG(@[0|90|0]SD,*)</to>
    </rule>
    <rule>
      <description>從慢栱插入散枓(右)</description>
      <from>MG(*,*)</from>
      <to>MG(*,@[0|90|0]SD)</to>
    </rule>
    <rule>
      <description>從華栱插入偷心華栱</description>
      <from>@[0|90|0]HG(*,*)=MG(*,*)</from>
      <to>@[0|90|0]HG(*,*)=MG(*,*)=SHHG(*,*,*)</to>
    </rule>
    <rule>
      <description>從偷心華栱插入交互枓</description>
      <from>SHHG(*,*,*)</from>
      <to>SHHG(*,*,@[0|90|0]JHD)</to>
    </rule>
    <rule>
      <description>從偷心華栱插入連珠枓</description>
      <from>SHHG(*,*,*)</from>
      <to>SHHG(*,LJD,*)</to>
    </rule>
    <rule>
      <description>從交互枓插入瓜子栱</description>
      <from>SHHG(*,*,@[0|90|0]JHD)</from>
      <to>SHHG(*,*,@[0|90|0]JHD=GJG(*,*))</to>
    </rule>
    <rule>
      <description>從偷心華栱插入慢拱</description>
      <from>SHHG(*,*,*)</from>
      <to>SHHG(*,*,*)=MG(*,*)</to>
    </rule>
    <rule>
      <description>從連珠枓插入華革木契</description>
      <from>LJD</from>
      <to>LJD=GHMCH(*)</to>
    </rule>
    <rule>
      <description>從華革木契插入交互枓</description>
      <from>GHMCH(*)</from>
      <to>GHMCH(@[0|90|0]JHD)</to>
    </rule>
    <rule>
      <description>從偷心華栱插入柱頭方</description>
      <from>SHHG(*,*,*)</from>
      <to>SHHG(JTF,*,*)</to>
    </rule>
    <rule>
      <description>從柱頭方栱插入反騎槽檐栱</description>
      <from>SHHG(JTF,*,*)</from>
      <to>SHHG(JTF=NCTJG(*,*,*) ,*,*)</to>
    </rule>
    <rule>
      <description>從反騎槽檐插入交互枓</description>
      <from>NCTJG(*,*,*)</from>
      <to>NCTJG(*,*,@[0|90|0]JHD)</to>
    </rule>
    <rule>
      <description>從反騎槽檐插入散枓</description>
      <from>NCTJG(*,*,*)</from>
      <to>NCTJG(*,*,*)=@[0|90|0]SD</to>
    </rule>
    <rule>
      <description>從交互枓插入瓜子栱</description>
      <from>NCTJG(*,*,@[0|90|0]JHD)</from>
      <to>NCTJG(*,*,@[0|90|0]JHD=GJG(*,*))</to>
    </rule>
    <rule>
      <description>從反騎槽檐插入慢栱</description>
      <from>NCTJG(*,*,*)</from>
      <to>NCTJG(*,MG(*,*),*)</to>
    </rule>
    <rule>
      <description>從反騎槽檐插入柱頭方</description>
      <from>NCTJG(*,*,*)</from>
      <to>NCTJG(JTF_B(*,*),*,*)</to>
    </rule>
    <rule>
      <description>從柱頭方插入散枓(左)</description>
      <from>JTF_B(*,*)</from>
      <to>JTF_B(@[0|90|0]SD,*)</to>
    </rule>
    <rule>
      <description>從柱頭方插入散枓(右)</description>
      <from>JTF_B(*,*)</from>
      <to>JTF_B(*,@[0|90|0]SD)</to>
    </rule>
    <rule>
      <description>從散枓插入慢栱</description>
      <from>NCTJG(*,*,*)=@[0|90|0]SD</from>
      <to>NCTJG(*,*,*)=@[0|90|0]SD=MG(*,*)</to>
    </rule>
    <rule>
      <description>從華革木契插入慢栱</description>
      <from>GHMCH(*)</from>
      <to>GHMCH(*)=MG(*,*)</to>
    </rule>
    <rule>
      <description>從柱頭方插入裏耍頭(偷心)</description>
      <from>NCTJG(JTF_B(*,*),*,*)</from>
      <to>NCTJG(JTF_B(*,*)=LSTL_B(*,*,*,*),*,*)</to>
    </rule>
    <rule>
      <description>從裏耍頭(偷心)插入散枓</description>
      <from>LSTL_B(*,*,*,*)</from>
      <to>LSTL_B(@[0|90|0]SD,*,*,*)</to>
    </rule>
    <rule>
      <description>從散枓插入平棊方</description>
      <from>LSTL_B(@[0|90|0]SD,*,*,*)</from>
      <to>LSTL_B(@[0|90|0]SD=PCF,*,*,*)</to>
    </rule>
    <rule>
      <description>從裏耍頭(偷心)插入柱頭方(前)</description>
      <from>LSTL_B(*,*,*,*)</from>
      <to>LSTL_B(*,JTF,*,*)</to>
    </rule>
    <rule>
      <description>從裏耍頭(偷心)插入柱頭方(中)</description>
      <from>LSTL_B(*,JTF,*,*)</from>
      <to>LSTL_B(*,JTF,*,*)=JTF</to>
    </rule>
    <rule>
      <description>從裏耍頭(偷心)插入柱頭方(中後)</description>
      <from>LSTL_B(*,JTF,*,*)=JTF</from>
      <to>LSTL_B(*,JTF,JTF,*)=JTF</to>
    </rule>
    <rule>
      <description>從裏耍頭(偷心)插入柱頭方(後)</description>
      <from>LSTL_B(*,JTF,JTF,*)=JTF</from>
      <to>LSTL_B(*,JTF,JTF,JTF_B(*,*))=JTF</to>
    </rule>
    <rule>
      <description>從交互枓插入瓜子栱</description>
      <from>GHMCH(@[0|90|0]JHD)</from>
      <to>GHMCH(@[0|90|0]JHD=GJG(*,*))</to>
    </rule>
    <rule>
